{
  "scripts": {
    "build": "./node_modules/.bin/webpack --env.NODE_ENV=production",
    "start": "./node_modules/.bin/webpack --env.NODE_ENV=dev --watch",
    "test": "./node_modules/.bin/jest",
    "svg": "svgr --no-dimensions -d assets/react/components/ui/svg/ assets/react/static/svg/"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "> 1%",
              "safari >= 10"
            ],
            "node": "current"
          },
          "useBuiltIns": true
        }
      ],
      "react",
      "stage-3",
      "jest"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-decorators-legacy"
    ],
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "jest": true,
      "node": true
    }
  },
